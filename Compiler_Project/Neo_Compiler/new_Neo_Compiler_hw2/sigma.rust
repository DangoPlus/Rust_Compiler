/* Sigma.rust
 *
 * Compute sum = 1 + 2 + ... + n
 */

fn main() {
  // variables
  let n = 10;
  let k:float = 7.5;
  let mut x[int,10];
  let mut y[bool,5];
  let mut z[float,100];
  let mut sum = 0;
  let mut index = 0;
    
  while (index <= n) {
    sum = sum + index;
    index = index + 1;
  } 
  print "The sum is ";
  println sum;
}
